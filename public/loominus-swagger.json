{
  "openapi": "3.0.2",
  "servers": [
    {
      "url": "https://public-api.loominus.ai"
    }
  ],
  "info": {
    "description": "Don't have an API Key? [[Register]](https://public.loominus.ai/register)",
    "title": "Loominus API",
    "version": "1.0.0"
  },
  "paths": {
    "/teraport/v1": {
      "post": {
        "summary": "Stream event to its corresponding Data Stream's Staging Table.",
        "description": "Send a JSON payload to Teraport to ingest and append to its corresponding Data Stream's Staging Table.",
        "parameters": [
          {
            "in": "header",
            "name": "X-Api-Key",
            "schema": {
              "type": "string",
              "description": "API access key"
            },
            "required": true
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "event_type": {
                    "type": "string",
                    "description": "Data stream name"
                  },
                  "environment": {
                    "type": "string",
                    "description": "Staging environment",
                    "enum": [
                      "test",
                      "production"
                    ]
                  },
                  "event_source": {
                    "type": "string",
                    "description": "How the event is sent to Teraport",
                    "enum": [
                      "api",
                      "test",
                      "ui"
                    ]
                  },
                  "payload": {
                    "type": "string",
                    "description": "JSON event payload"
                  },
                  "partition_key": {
                    "type": "string",
                    "description": "Optional partition key to help with distributing streamed data in a multi-shard deployment. If not given, Teraport will assign a random UUID as the partition key."
                  }
                },
                "required": [
                  "event_type",
                  "environment",
                  "event_source",
                  "payload"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "result": {
                      "type": "object",
                      "properties": {
                        "filter_messages": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "properties": {
                              "filter_type": {
                                "type": "string"
                              },
                              "message": {
                                "type": "string"
                              },
                              "sucess_rows": {
                                "type": "integer"
                              },
                              "success_columns": {
                                "type": "array",
                                "items": {
                                  "type": "string"
                                }
                              },
                              "error_columns": {
                                "type": "array",
                                "items": {
                                  "type": "string"
                                }
                              }
                            }
                          }
                        },
                        "partial_errors": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "properties": {
                              "error": {
                                "type": "string"
                              },
                              "payload": {
                                "type": "string"
                              }
                            }
                          }
                        }
                      }
                    },
                    "errorMessage": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "x-code-samples": [
          {
            "lang": "Curl",
            "source": "curl -d '{\"event_type\":\"MyDataStream\",\"environment\":\"test\",\"event_source\":\"api\",\"payload\":\"{\\\"field1\\\": \\\"value1\\\", \\\"field2\\\": \\\"value2\\\", \\\"field3\\\": \\\"value3\\\"}\"}' -H \"X-Api-Key: Replace with Teraport API Key\" -H \"Content-Type: application/json\" -X POST https://public-api.loominus.ai/teraport/v1\n                    "
          }
        ]
      }
    },
