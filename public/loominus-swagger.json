        "x-code-samples": [
          {
            "lang": "Curl",
            "source": "curl -H \"X-Api-Key: Replace with ModOps API Key\" -X GET https://public-api.loominus.ai/predictive/v1/modops/models/deployed/:id\n                    "
          }
        ]
      },
      "post": {
        "summary": "Make predictions",
        "description": "Send input data to a Predictive Model to make predictions. **It is the caller's responsibility to store the returned request_id of each prediction in order to inform ModOps of the prediction's actual outcome.**",
        "parameters": [
          {
            "in": "header",
            "name": "X-Api-Key",
            "schema": {
              "type": "string",
              "description": "API access key"
            },
            "required": true
          },
          {
            "in": "path",
            "name": "model_id",
            "schema": {
              "type": "string"
            },
            "required": true,
            "description": "UUID of deployed Predictive Model"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "data": {
                    "type": "array",
                    "description": "Array of arrays of values corresponding to the Predictive Model's feature columns. Order of the values matters!!!",
                    "items": {
                      "type": "array",
                      "items": {
                        "anyOf": [
                          {
                            "type": "string"
                          },
                          {
                            "type": "number"
                          },
                          {
                            "type": "integer"
                          },
                          {
                            "type": "boolean"
                          }
                        ]
                      }
                    }
                  },
                  "secondary_ids": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                },
                "required": [
                  "data"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "result": {
                      "type": "object",
                      "properties": {
                        "predicted_probabilities": {
                          "type": "array",
                          "description": "Only pertains to Classifier models.",
                          "items": {
                            "type": "array",
                            "items": {
                              "type": "number"
                            }
                          }
                        },
                        "predicted_value": {
                          "type": "array",
                          "items": {
                            "oneOf": [
                              {
                                "type": "string"
                              },
                              {
                                "type": "number"
                              },
                              {
                                "type": "boolean"
                              }
                            ]
                          }
                        },
                        "request_id": {
                          "type": "array",
                          "items": {
                            "type": "string"
                          }
                        },
                        "model_id": {
                          "type": "string"
                        },
                        "prediction_datetime": {
                          "type": "string"
                        },
                        "secondary_id": {
                          "type": "array",
                          "items": {
                            "type": "string"
                          }
                        },
                        "classes": {
                          "type": "array",
                          "description": "Only pertains to Classifier models.",
                          "items": {
                            "oneOf": [
                              {
                                "type": "boolean"
                              },
                              {
                                "type": "string"
                              }
                            ]
                          }
                        }
                      }
                    },
                    "errorMessage": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "x-code-samples": [
          {
            "lang": "Curl",
            "source": "curl -d '{\"data\":[\"feature_column_1_value\", \"feature_column_2_value\", \"feature_column_3_value\"]}' -H \"X-Api-Key: Replace with ModOps API Key\" -H \"Content-Type: application/json\" -X POST https://public-api.loominus.ai/predictive/v1/modops/models/deployed/:id\n                    "
          }
        ]
      },
      "put": {
        "summary": "Inform ModOps of the actual outcomes of predictions",
        "description": "Send the actual value that is the outcome for a specific prediction request.",
        "parameters": [
          {
            "in": "header",
            "name": "X-Api-Key",
            "schema": {
              "type": "string",
              "description": "API access key"
            },
            "required": true
          },
          {
            "in": "path",
            "name": "model_id",
            "schema": {
              "type": "string"
            },
            "required": true,
            "description": "UUID of deployed Predictive Model"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "request_id": {
                      "type": "string"
                    },
                    "actual_value": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "result": {
                      "type": "boolean"
                    },
                    "errorMessage": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "x-code-samples": [
          {
            "lang": "Curl",
            "source": "curl -d '{\"request_id\":\"prediction_request_id\", \"actual_value\":\"actual_outcome\"}' -H \"X-Api-Key: Replace with ModOps API Key\" -H \"Content-Type: application/json\" -X PUT https://public-api.loominus.ai/predictive/v1/modops/models/deployed/:id\n                    "
          }
        ]
      }
